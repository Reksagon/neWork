<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RangeOfResistanceActivity">

    <View
        android:id="@+id/arrow5"
        android:layout_width="16dp"
        android:layout_height="26dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/arrow_red"
        android:translationZ="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/txt_range_of_resistance5"
        app:layout_constraintEnd_toStartOf="@+id/txt_range_of_resistance5"
        app:layout_constraintTop_toTopOf="@+id/txt_range_of_resistance5"
        app:layout_constraintVertical_bias="0.4" />

    <View
        android:id="@+id/bar_expand5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:background="@drawable/bar_expand"
        android:translationZ="50dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/bttn_cancel5"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/ic_cancel"
        android:translationZ="70dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/bar_expand5"
        app:layout_constraintTop_toTopOf="@+id/bttn_bar5" />

    <TextView
        android:id="@+id/txt_device_name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="120dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Device name"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:translationZ="70dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_ip_number5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_regular"
        android:text="IP Number"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:translationZ="70dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toBottomOf="@+id/txt_device_name5" />

    <TextView
        android:id="@+id/txt_resistance_ratio5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Resistance ratio"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:translationZ="70dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toBottomOf="@+id/txt_ip_number5" />

    <TextView
        android:id="@+id/txt_range_of_resistance5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Range of resistance"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:translationZ="70dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toBottomOf="@+id/txt_resistance_ratio5" />

    <TextView
        android:id="@+id/txt_range_of_motion5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Range of motion"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:translationZ="70dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toBottomOf="@+id/txt_range_of_resistance5" />

    <com.github.nikartm.button.FitButton
        android:id="@+id/bttn_save"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:enabled="true"
        android:gravity="center|center_vertical"
        app:fb_backgroundColor="#DF2323"
        app:fb_cornerRadius="40dp"
        app:fb_fontFamilyRes="@font/poppins_semibold"
        app:fb_shape="rectangle"
        app:fb_text="SAVE"
        app:fb_textColor="#FFF"
        app:fb_textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline26"
        app:layout_constraintVertical_bias="0.9" />

    <View
        android:id="@+id/view5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/background_choosetype"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/imageView6"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:background="@drawable/vector_bar"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/bttn_bar5"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:background="@drawable/ic_bar"
        android:translationZ="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4" />

    <EditText
        android:id="@+id/editText_minimum"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/edit_text_style"
        android:ems="10"
        android:gravity="center"
        android:fontFamily="@font/poppins_medium"
        android:inputType="number"
        android:paddingStart="50dp"
        android:paddingTop="5dp"
        android:paddingEnd="50dp"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline26"
        app:layout_constraintEnd_toStartOf="@+id/guideline24"
        app:layout_constraintStart_toStartOf="@+id/guideline23"
        app:layout_constraintTop_toTopOf="@+id/guideline25"
        app:layout_constraintVertical_bias="0.55" />

    <EditText
        android:id="@+id/editText_maxmimum"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/edit_text_style"
        android:ems="10"
        android:gravity="center"
        android:fontFamily="@font/poppins_medium"
        android:inputType="number"
        android:paddingStart="50dp"
        android:paddingTop="5dp"
        android:paddingEnd="50dp"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline26"
        app:layout_constraintEnd_toStartOf="@+id/guideline28"
        app:layout_constraintStart_toStartOf="@+id/guideline27"
        app:layout_constraintTop_toTopOf="@+id/guideline25"
        app:layout_constraintVertical_bias="0.55" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="Range of resistance"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline25"
        app:layout_constraintEnd_toStartOf="@+id/guideline28"
        app:layout_constraintStart_toStartOf="@+id/guideline23"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintVertical_bias="0.65" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.48" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6" />

    <View
        android:id="@+id/view8"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/guideline26"
        app:layout_constraintEnd_toStartOf="@+id/guideline27"
        app:layout_constraintStart_toEndOf="@+id/editText_minimum"
        app:layout_constraintTop_toTopOf="@+id/guideline25" />
</androidx.constraintlayout.widget.ConstraintLayout>