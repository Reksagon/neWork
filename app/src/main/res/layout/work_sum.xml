<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:rotation="0">

	<View
		android:id="@+id/view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/background_train"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="1.0" />

	<View
		android:id="@+id/cancel"
		android:layout_width="175dp"
		android:layout_height="50dp"
		android:layout_marginStart="30dp"
		android:layout_marginTop="20dp"
		android:background="@drawable/bttn_cancel"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<View
		android:id="@+id/img_logo"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginStart="110dp"
		android:layout_marginTop="110dp"
		android:layout_marginEnd="110dp"
		android:layout_marginBottom="110dp"
		android:background="@drawable/logo_work_sum"
		app:layout_constraintBottom_toTopOf="@+id/guideline41"
		app:layout_constraintEnd_toEndOf="@+id/main_round"
		app:layout_constraintStart_toStartOf="@+id/main_round"
		app:layout_constraintTop_toTopOf="@+id/main_round" />

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline40"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		app:layout_constraintGuide_percent="0.15" />

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline41"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		app:layout_constraintGuide_percent="0.85" />

	<me.tankery.lib.circularseekbar.CircularSeekBar
		android:id="@+id/main_round"
		android:layout_width="0dp"
		android:layout_height="0dp"
		app:cs_circle_color  = "@android:color/transparent"
		app:layout_constraintBottom_toTopOf="@+id/guideline41"
		app:layout_constraintDimensionRatio="1:1"
		app:layout_constraintEnd_toEndOf="@+id/view"
		app:layout_constraintHorizontal_bias="0.491"
		app:layout_constraintStart_toStartOf="@+id/view"
		app:layout_constraintTop_toTopOf="@+id/guideline40"
		app:layout_constraintVertical_bias="0.0">

	</me.tankery.lib.circularseekbar.CircularSeekBar>

	<me.tankery.lib.circularseekbar.CircularSeekBar
		android:id="@+id/lume_round"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginEnd="-11dp"
		android:visibility="invisible"
		app:cs_circle_color="@android:color/transparent"
		app:layout_constraintBottom_toBottomOf="@id/guidline_4"
		app:layout_constraintDimensionRatio="1:1"
		app:layout_constraintEnd_toEndOf="@+id/img_lume"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="@id/guidline_3">

	</me.tankery.lib.circularseekbar.CircularSeekBar>

	<me.tankery.lib.circularseekbar.CircularSeekBar
		android:id="@+id/range_round"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginStart="-9dp"
		android:layout_marginTop="5dp"
		android:layout_marginBottom="5dp"
		android:visibility="invisible"
		app:cs_circle_color="@android:color/transparent"
		app:layout_constraintBottom_toTopOf="@+id/guidline_2"
		app:layout_constraintDimensionRatio="1:1"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="@+id/img_range"
		app:layout_constraintTop_toBottomOf="@+id/guidline_1">

	</me.tankery.lib.circularseekbar.CircularSeekBar>

	<View
		android:id="@+id/img_logo_2"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginStart="90dp"
		android:layout_marginTop="90dp"
		android:layout_marginEnd="90dp"
		android:layout_marginBottom="90dp"
		android:background="@drawable/round_1_sum"
		app:layout_constraintBottom_toBottomOf="@+id/main_round"
		app:layout_constraintEnd_toEndOf="@+id/main_round"
		app:layout_constraintStart_toStartOf="@+id/main_round"
		app:layout_constraintTop_toTopOf="@+id/main_round" />

	<View
		android:id="@+id/img_lume"
		android:layout_width="100dp"
		android:layout_height="100dp"
		android:layout_marginBottom="70dp"
		android:background="@drawable/sum_field"
		android:visibility="invisible"
		app:layout_constraintBottom_toTopOf="@+id/img_logo_2"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="@+id/guideline40"
		tools:visibility="visible" />

	<View
		android:id="@+id/img_power"
		android:layout_width="100dp"
		android:layout_height="100dp"
		android:layout_marginEnd="20dp"
		android:background="@drawable/sum_field"
		android:visibility="invisible"
		app:layout_constraintEnd_toStartOf="@+id/img_logo_2"
		app:layout_constraintTop_toTopOf="@+id/guideline43"
		tools:visibility="visible" />

	<View
		android:id="@+id/img_range"
		android:layout_width="100dp"
		android:layout_height="100dp"
		android:layout_marginStart="20dp"
		android:background="@drawable/sum_field"
		android:visibility="invisible"
		app:layout_constraintStart_toEndOf="@+id/img_logo_2"
		app:layout_constraintTop_toTopOf="@+id/guideline43"
		tools:visibility="visible" />

	<me.tankery.lib.circularseekbar.CircularSeekBar
		android:id="@+id/power_round"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginTop="5dp"
		android:layout_marginEnd="-10dp"
		android:layout_marginBottom="5dp"
		android:visibility="invisible"
		app:cs_circle_color="@android:color/transparent"
		app:layout_constraintBottom_toTopOf="@+id/guidline_2"
		app:layout_constraintDimensionRatio="1:1"
		app:layout_constraintEnd_toEndOf="@+id/img_power"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/guidline_1">

	</me.tankery.lib.circularseekbar.CircularSeekBar>

	<View
		android:id="@+id/guidline_1"
		android:layout_width="wrap_content"
		android:layout_height="1dp"
		android:layout_marginBottom="15dp"
		app:layout_constraintBottom_toTopOf="@+id/img_power"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent" />

	<View
		android:id="@+id/guidline_3"
		android:layout_width="wrap_content"
		android:layout_height="1dp"
		android:layout_marginBottom="10dp"
		app:layout_constraintBottom_toTopOf="@+id/img_lume"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent" />

	<View
		android:id="@+id/guidline_2"
		android:layout_width="wrap_content"
		android:layout_height="1dp"
		android:layout_marginTop="15dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/img_power" />

	<View
		android:id="@+id/guidline_4"
		android:layout_width="wrap_content"
		android:layout_height="1dp"
		android:layout_marginTop="10dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/img_lume" />

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline43"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		app:layout_constraintGuide_percent="0.55" />

	<TextView
		android:id="@+id/lume_score"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginEnd="10dp"
		android:fontFamily="@font/poppins_semibold"
		android:text="0"
		android:textColor="#FFFFFF"
		android:textSize="24sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toBottomOf="@+id/img_lume"
		app:layout_constraintEnd_toEndOf="@+id/img_lume"
		app:layout_constraintStart_toStartOf="@+id/img_lume"
		app:layout_constraintTop_toTopOf="@+id/img_lume"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/power_score"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppins_semibold"
		android:text="0"
		android:textColor="#FFFFFF"
		android:textSize="28sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toBottomOf="@+id/img_power"
		app:layout_constraintEnd_toEndOf="@+id/img_power"
		app:layout_constraintStart_toStartOf="@+id/img_power"
		app:layout_constraintTop_toTopOf="@+id/img_power"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/range_score"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppins_semibold"
		android:text="0"
		android:textColor="#FFFFFF"
		android:textSize="28sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toBottomOf="@+id/img_range"
		app:layout_constraintEnd_toEndOf="@+id/img_range"
		app:layout_constraintStart_toStartOf="@+id/img_range"
		app:layout_constraintTop_toTopOf="@+id/img_range"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/lume_txt1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="5dp"
		android:layout_marginBottom="5dp"
		android:fontFamily="@font/poppins_medium"
		android:text="KG"
		android:textColor="#FFFFFF"
		android:textSize="14sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toBottomOf="@+id/lume_score"
		app:layout_constraintStart_toEndOf="@+id/lume_score"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/range_txt1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="5dp"
		android:layout_marginBottom="5dp"
		android:fontFamily="@font/poppins_medium"
		android:text="%"
		android:textColor="#FFFFFF"
		android:textSize="14sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toTopOf="@+id/range_txt2"
		app:layout_constraintStart_toEndOf="@+id/range_score"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/power_txt1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="5dp"
		android:layout_marginBottom="5dp"
		android:fontFamily="@font/poppins_medium"
		android:text="%"
		android:textColor="#FFFFFF"
		android:textSize="14sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toTopOf="@+id/power_txt2"
		app:layout_constraintStart_toEndOf="@+id/power_score"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/lume_txt2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppins_medium"
		android:text="VOLUME"
		android:textColor="#FFFFFF"
		android:textSize="10sp"
		android:visibility="invisible"
		app:layout_constraintBottom_toBottomOf="@+id/img_lume"
		app:layout_constraintEnd_toEndOf="@+id/img_lume"
		app:layout_constraintStart_toStartOf="@+id/img_lume"
		app:layout_constraintTop_toBottomOf="@+id/lume_score"
		app:layout_constraintVertical_bias="0.0"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/power_txt2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="-5dp"
		android:fontFamily="@font/poppins_medium"
		android:text="POWER(RM)"
		android:textColor="#FFFFFF"
		android:textSize="10sp"
		android:visibility="invisible"
		app:layout_constraintEnd_toEndOf="@+id/img_power"
		app:layout_constraintStart_toStartOf="@+id/img_power"
		app:layout_constraintTop_toBottomOf="@+id/power_score"
		tools:visibility="visible" />

	<TextView
		android:id="@+id/range_txt2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="-5dp"
		android:fontFamily="@font/poppins_medium"
		android:text="RANGE"
		android:textColor="#FFFFFF"
		android:textSize="10sp"
		android:visibility="invisible"
		app:layout_constraintEnd_toEndOf="@+id/img_range"
		app:layout_constraintStart_toStartOf="@+id/img_range"
		app:layout_constraintTop_toBottomOf="@+id/range_score"
		tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>
