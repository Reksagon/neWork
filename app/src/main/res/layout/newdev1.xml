<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="@drawable/bg0807"
		android:orientation="vertical"
		android:weightSum="10">
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="20dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:weightSum="100"
			android:layout_weight="2">
			<LinearLayout
				android:id="@+id/linear18"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="30">
				<LinearLayout
					android:id="@+id/linear15"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:padding="8dp"
					android:background="@drawable/xicon"
					android:gravity="left"
					android:orientation="horizontal"
					android:layout_gravity="left|top"/>
			</LinearLayout>
			<TextView
				android:id="@+id/top_minus"
				android:layout_width="5dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="-"
				android:textSize="20sp"
				android:textColor="#FFFFFF"
				android:layout_weight="5"/>
			<LinearLayout
				android:id="@+id/lin_resist"
				android:layout_width="30dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:weightSum="2"
				android:layout_weight="30">
				<LinearLayout
					android:id="@+id/linear11"
					android:layout_width="10dp"
					android:layout_height="match_parent"
					android:gravity="right|center_vertical"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/resist"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="right|center_vertical"
						android:text="40"
						android:textSize="40sp"
						android:textColor="#FFFFFF"/>
					<TextView
						android:id="@+id/textview10"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="KG"
						android:textSize="18sp"
						android:textColor="#808090"/>
				</LinearLayout>
				<TextView
					android:id="@+id/txt_resist"
					android:layout_width="10dp"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right|center_vertical"
					android:text="התנגדות:"
					android:textSize="20sp"
					android:textColor="#808090"
					android:layout_weight="1"/>
			</LinearLayout>
			<TextView
				android:id="@+id/top_plus"
				android:layout_width="5dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="+"
				android:textSize="20sp"
				android:textColor="#FFFFFF"
				android:layout_weight="5"/>
			<LinearLayout
				android:id="@+id/linear14"
				android:layout_width="1dp"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="30"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="10dp"
			android:orientation="horizontal"
			android:layout_weight="1">
			<SeekBar
				android:id="@+id/sb_top"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:layout_marginBottom="10dp"
				android:padding="8dp"
				android:background="@drawable/bar3x"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="match_parent"
			android:layout_height="20dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:weightSum="100"
			android:layout_weight="2">
			<TextView
				android:id="@+id/bottom_minus"
				android:layout_width="5dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="-"
				android:textSize="20sp"
				android:textColor="#FFFFFF"
				android:layout_weight="5"/>
			<LinearLayout
				android:id="@+id/lin_reps"
				android:layout_width="30dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:gravity="right|center_vertical"
				android:orientation="horizontal"
				android:weightSum="2"
				android:layout_weight="30">
				<TextView
					android:id="@+id/reps"
					android:layout_width="10dp"
					android:layout_height="match_parent"
					android:gravity="center_horizontal|center_vertical"
					android:text="--"
					android:textSize="40sp"
					android:textColor="#FF0000"
					android:layout_weight="1"/>
				<TextView
					android:id="@+id/txt_reps"
					android:layout_width="10dp"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right|center_vertical"
					android:text="חזרות:"
					android:textSize="20sp"
					android:textColor="#808090"
					android:layout_weight="1"/>
			</LinearLayout>
			<TextView
				android:id="@+id/bottom_plus"
				android:layout_width="5dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="+"
				android:textSize="20sp"
				android:textColor="#FFFFFF"
				android:layout_weight="5"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="match_parent"
			android:layout_height="10dp"
			android:orientation="horizontal"
			android:layout_weight="1">
			<SeekBar
				android:id="@+id/sb_bottom"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:layout_marginBottom="10dp"
				android:padding="8dp"
				android:background="@drawable/bar3x"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear7"
			android:layout_width="match_parent"
			android:layout_height="20dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:weightSum="100"
			android:layout_weight="3">
			<TextView
				android:id="@+id/remset"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="הסר\nסט"
				android:textSize="18sp"
				android:textColor="#808090"
				android:layout_weight="10"/>
			<TextView
				android:id="@+id/set1"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="1"
				android:textSize="40sp"
				android:textColor="#FFFFFF"
				android:layout_weight="10"/>
			<TextView
				android:id="@+id/set2"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="2"
				android:textSize="40sp"
				android:textColor="#FFFFFF"
				android:layout_weight="10"/>
			<TextView
				android:id="@+id/set3"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="3"
				android:textSize="40sp"
				android:textColor="#FFFFFF"
				android:layout_weight="10"/>
			<TextView
				android:id="@+id/set4"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="4"
				android:textSize="40sp"
				android:textColor="#FFFFFF"
				android:layout_weight="10"/>
			<TextView
				android:id="@+id/set5"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:background="#9E9E9E"
				android:gravity="center_horizontal|center_vertical"
				android:text="5"
				android:textSize="40sp"
				android:textColor="#FFFFFF"
				android:layout_weight="10"/>
			<TextView
				android:id="@+id/addset"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_margin="8dp"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="הוסף\nסט"
				android:textSize="18sp"
				android:textColor="#808090"
				android:layout_weight="10"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear19"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal|bottom"
			android:orientation="horizontal"
			android:weightSum="100"
			android:layout_weight="1">
			<Button
				android:id="@+id/btn_OK"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#D50000"
				android:text="שמירת נתוני תרגיל"
				android:textSize="20sp"
				android:textColor="#FFFFFF"
				android:layout_weight="30"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
